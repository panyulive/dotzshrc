# グローバルエイリアス
alias -g L='| less'
alias -g G='| grep'

alias em='/Applications/Emacs.app/Contents/MacOS/Emacs -q -nw -l ~/.emacs.d/default.elc ./'
alias emacs='/Applications/Emacs.app/Contents/MacOS/Emacs -nw ./'

