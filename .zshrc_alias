# エイリアス

alias la='ls -a'
alias ll='ls -l'

alias rm='rm -f'
alias cp='cp -f'
alias mv='mv -f'

alias mkdir='mkdir -p'

alias be='bundle exec'
#RSS
alias RSS='say -r 1 "あーるえすえす。  あーるえすえす。 あーるえすえす"'
# sudo の後のコマンドでエイリアスを有効にする
